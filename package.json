{
  "name": "nodejs-api",
  "version": "0.1.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "src/main.ts",
  "scripts": {
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "NODE_PATH=src/ NODE_ENV=development nodemon main.js --exec babel-node --presets es2015",
    "start:prod": "NODE_PATH=src/ NODE_ENV=production nodemon main.js --exec babel-node --presets es2015",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "dolgopolova/node.js_example",
  "author": "Tanya Dolgopolova",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.5.2",
    "config": "^3.3.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
